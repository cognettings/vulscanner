<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <appSettings />
  <system.webServer>
    <handlers>
      <add
        name="UnsafeAllMethods"
        path="*."
        verb="*"
      />
      <add
        name="UnsafeSpecificMethods"
        path="*."
        verb="TRACE,DEBUG,POST,DELETE"
      />
    </handlers>
  </system.webServer>
</configuration>
